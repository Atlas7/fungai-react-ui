{
  "name": "fungai-react-ui",
  "version": "1.1.0",
  "description": "A ReactJS UI for a Fungi Image Classification Project. A proof of concept",
  "main": "server.js",
  "scripts": {
    "dev": "webpack-dev-server --open --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "start": "npm run build && NODE_ENV=production node server.js",
    "test": "mocha --require babel-core/register --require ./test/testHelper.js  --recursive",
    "test:watch": "npm run test -- --watch"
  },
  "engines": {
    "node": "8.9.x",
    "npm": "5.5.x"
  },
  "babel": {
    "presets": [
      "env",
      "react"
    ],
    "plugins": [
      "transform-class-properties",
      "syntax-async-functions",
      "transform-regenerator"
    ]
  },
  "author": "Johnny Chan",
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "bootstrap": "^3.3.7",
    "es6-promise": "^4.1.1",
    "express": "^4.16.2",
    "immutable": "^3.8.2",
    "isomorphic-fetch": "^2.2.1",
    "prop-types": "^15.6.0",
    "query-string": "^5.0.1",
    "react": "^16.0.0",
    "react-bootstrap": "^0.31.5",
    "react-dom": "^16.0.0",
    "react-router-dom": "^4.2.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "chai-immutable": "^1.6.0",
    "clean-webpack-plugin": "^0.1.17",
    "css-loader": "^0.28.7",
    "csv-loader": "^2.1.1",
    "file-loader": "^1.1.5",
    "firebase-tools": "^3.14.0",
    "html-webpack-plugin": "^2.30.1",
    "mocha": "^4.0.1",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4",
    "webpack-merge": "^4.1.1",
    "xml-loader": "^1.2.1"
  }
}
