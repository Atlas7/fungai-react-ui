{
  "name": "fungai-react-ui",
  "version": "1.0.0",
  "description": "A ReactJS UI for a Fungi Image Classification Project. A proof of concept",
  "main": "index.js",
  "scripts": {
    "start-app": "webpack-dev-server --open",
    "start-api": "json-server --watch fakeServer/db.json",
    "build": "NODE_ENV='production' webpack -p",
    "build-dev": "webpack",
    "deploy": "npm run build && firebase deploy",
    "firebase-init": "firebase login && firebase init",
    "test": "mocha --require babel-core/register --require ./test/testHelper.js  --recursive",
    "test:watch": "npm run test -- --watch"
  },
  "babel": {
    "presets": [
      "env",
      "react"
    ],
    "plugins": [
      "transform-class-properties",
      "syntax-async-functions",
      "transform-regenerator"
    ]
  },
  "author": "Johnny Chan",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.17.0",
    "babel-polyfill": "^6.26.0",
    "bootstrap": "^3.3.7",
    "es6-promise": "^4.1.1",
    "immutable": "^3.8.2",
    "isomorphic-fetch": "^2.2.1",
    "prop-types": "^15.6.0",
    "query-string": "^5.0.1",
    "react": "^16.0.0",
    "react-bootstrap": "^0.31.5",
    "react-dom": "^16.0.0",
    "react-router-dom": "^4.2.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "chai-immutable": "^1.6.0",
    "css-loader": "^0.28.7",
    "file-loader": "^1.1.5",
    "firebase-tools": "^3.14.0",
    "html-webpack-plugin": "^2.30.1",
    "json-server": "^0.12.1",
    "mocha": "^4.0.1",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4"
  }
}
